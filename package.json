{
  "name": "@jkallunki/css-in-to-js",
  "version": "1.0.0",
  "description": "CSS to CSS-in-JS and Styled converter",
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha --require @babel/polyfill --require @babel/register 'src/*.test.js'",
    "test:watch": "mocha -w --require @babel/polyfill --require @babel/register 'src/*.test.js'",
    "prebuild": "rimraf dist",
    "build": "babel src --out-dir dist --ignore 'src/*.test.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/J-Kallunki/css-in-to-js.git"
  },
  "keywords": [
    "css",
    "cssinjs"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "author": "Joonas Kallunki <jkallunki@gmail.com> (https://jkallunki.net/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/J-Kallunki/css-in-to-js/issues"
  },
  "homepage": "https://github.com/J-Kallunki/css-in-to-js#readme",
  "dependencies": {
    "clean-css": "4.2.1",
    "lodash": "4.17.11",
    "postcss": "7.0.5"
  },
  "devDependencies": {
    "@babel/cli": "7.1.5",
    "@babel/core": "7.1.6",
    "@babel/polyfill": "7.0.0",
    "@babel/preset-env": "7.1.6",
    "@babel/register": "7.0.0",
    "babel-plugin-inline-import": "3.0.0",
    "chai": "4.2.0",
    "mocha": "5.2.0",
    "mocha-loader": "2.0.0",
    "rimraf": "2.6.2",
    "webpack": "4.25.1",
    "webpack-cli": "3.1.2"
  }
}
